<% include header %>
<% if (entries.length) { %>
<% entries.forEach(function(entry) {
    console.log("Post published :" + entry.published)
    console.log("Post title :" + entry.title)
    console.log("Post content :" + entry.content)
    %>
<div class="panel panel-default">
<div class="panel-heading">
<div class="text-muted pull-right">
<%= entry.published %></div>
<%= entry.title %></div>
<div class="panel-body">
<%= entry.content %>
</div>
</div>
<% }) %>
<% } else { %>
No entries! <a href="/new-entry">Add one!</a>
<% } %>
<% include footer %>